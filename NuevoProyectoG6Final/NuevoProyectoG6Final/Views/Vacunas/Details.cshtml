@model Vet.DAL.Vacuna

@{
    ViewData["Title"] = "Details";
}
<h1>Detalles</h1>

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Vacuna</h5>
        <hr />

        <dl class="row">
            <div class="card-subtitle">
                <h6> @Html.DisplayNameFor(model => model.Nombre) </h6>
            </div>
            <p class="card-text">
                @Html.DisplayFor(model => model.Nombre)
            </p>
            <div class="card-subtitle">
                <h6> @Html.DisplayNameFor(model => model.TipoVacuna)</h6>
            </div>
            <p class="card-text">
                @Html.DisplayFor(model => model.TipoVacuna)
            </p>
            <div class="card-subtitle">
                <h6> @Html.DisplayNameFor(model => model.Producto)</h6>
            </div>
            <p class="card-text">
                @Html.DisplayFor(model => model.Producto)
            </p>
        </dl>
    </div>
    <div class="card-footer">
        <a asp-action="Edit" asp-route-id="@Model?.IdVacuna" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-outline-secondary">Atrás</a>
    </div>
</div>
